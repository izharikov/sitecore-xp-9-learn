@model IEnumerable<events.tac.local.Models.NavigationItem>
<div class="container">
    <ol class="breadcrumb">
        @foreach (var navItem in Model)
            {
            <li class="@(navItem.Active ? "active" : string.Empty)">
                <a href="@navItem.URL">@navItem.Title</a>
            </li>
        }
    </ol>
</div>
